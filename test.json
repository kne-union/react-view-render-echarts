          "props": {
            "span": 12
          },
          "children": [
            {
              "id": "Remote-13",
              "component": "Remote",
              "props": {
                "resource": "$getJSON"
              },
              "children": [
                {
                  "id": "EChart-12",
                  "component": "EChartGraph",
                  "props": {
                    "style": {
                      "width": "100%",
                      "height": "300px"
                    },
                    "options": {
                      "title": {
                        "text": "Les Miserables",
                        "subtext": "Circular layout",
                        "top": "bottom",
                        "left": "right"
                      },
                      "animationDurationUpdate": 1500,
                      "animationEasingUpdate": "quinticInOut",
                      "series": [
                        {
                          "name": "Les Miserables",
                          "type": "graph",
                          "layout": "circular",
                          "circular": {
                            "rotateLabel": true
                          },
                          "data": "$item.nodes",
                          "links": "$item.links",
                          "categories": "$item.categories",
                          "roam": true,
                          "label": {
                            "position": "right",
                            "formatter": "{b}"
                          },
                          "lineStyle": {
                            "color": "source",
                            "curveness": 0.3
                          }
                        }
                      ]
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
